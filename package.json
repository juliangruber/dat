{
  "name": "dat",
  "version": "11.3.1",
  "description": "Share datasets on the Internet",
  "keywords": [
    "data package",
    "dataset",
    "replication",
    "synchronization"
  ],
  "bin": {
    "dat": "bin/cli.js"
  },
  "scripts": {
    "install-precommit": "echo ./node_modules/.bin/standard > .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "standard": "standard",
    "test": "standard && tape tests/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/datproject/dat.git"
  },
  "author": "max ogden",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chalk": "^1.1.1",
    "dat-js": "^3.5.1",
    "datland-swarm-defaults": "^1.0.2",
    "discovery-swarm": "^4.0.2",
    "dns-discovery": "^5.3.4",
    "memdb": "^1.3.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pretty-bytes": "^4.0.2",
    "pump": "^1.0.1",
    "speedometer": "^1.0.0",
    "status-logger": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/datproject/dat/issues"
  },
  "homepage": "https://github.com/datproject/dat",
